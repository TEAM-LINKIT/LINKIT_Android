<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".signup.SignUpPartActivity"
    android:background="@color/white">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/view12"
            android:layout_marginTop="80dp"
            android:layout_width="19dp"
            android:layout_height="4dp"
            android:layout_marginStart="31dp"
            android:layout_marginLeft="31dp"
            android:background="@color/red_main"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/notosanskrbold"
            android:includeFontPadding="false"
            android:letterSpacing="-0.03"
            android:text="관심 파트"
            android:textColor="@color/black_content"
            android:textSize="20dp"
            app:layout_constraintStart_toStartOf="@+id/view12"
            app:layout_constraintTop_toBottomOf="@+id/view12" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/notosanskrregular"
            android:includeFontPadding="false"
            android:letterSpacing="-0.03"
            android:text="관심 파트를 한 가지 선택해주세요"
            android:textColor="@color/gray_content"
            android:textSize="12dp"
            app:layout_constraintStart_toStartOf="@+id/textView23"
            app:layout_constraintTop_toBottomOf="@+id/textView23" />

        <androidx.cardview.widget.CardView
            android:id="@+id/btn_plan"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="7dp"
            android:layout_marginRight="7dp"
            app:cardCornerRadius="10dp"
            android:outlineSpotShadowColor="@color/gray_shadow"
            android:outlineAmbientShadowColor="@color/gray_shadow"
            app:cardElevation="10dp"
            app:layout_constraintDimensionRatio="1:0.9855"
            app:layout_constraintEnd_toStartOf="@+id/btn_design"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textView24"
            app:layout_constraintWidth_percent="0.3833">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout14"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginTop="30dp"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_percent="0.3695">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/circleImageView2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@color/red_light_btn" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:src="@drawable/ic_plan"
                        app:layout_constraintBottom_toBottomOf="@+id/circleImageView2"
                        app:layout_constraintDimensionRatio="1:1"
                        app:layout_constraintEnd_toEndOf="@+id/circleImageView2"
                        app:layout_constraintStart_toStartOf="@+id/circleImageView2"
                        app:layout_constraintTop_toTopOf="@+id/circleImageView2"
                        app:layout_constraintWidth_percent="0.5490" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/notosanskrmedium"
                    android:includeFontPadding="false"
                    android:letterSpacing="-0.03"
                    android:text="기획"
                    android:textColor="@color/gray_content"
                    android:textSize="13dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout14" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/btn_design"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="7dp"
            android:layout_marginLeft="7dp"
            app:cardCornerRadius="10dp"
            android:outlineSpotShadowColor="@color/gray_shadow"
            android:outlineAmbientShadowColor="@color/gray_shadow"
            app:cardElevation="10dp"
            app:layout_constraintDimensionRatio="1:0.9855"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/btn_plan"
            app:layout_constraintTop_toTopOf="@+id/btn_plan"
            app:layout_constraintWidth_percent="0.3833">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout15"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginTop="30dp"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_percent="0.3695">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/circleImageView3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@color/red_light_btn" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:src="@drawable/ic_design"
                        app:layout_constraintBottom_toBottomOf="@+id/circleImageView3"
                        app:layout_constraintDimensionRatio="1:1"
                        app:layout_constraintEnd_toEndOf="@+id/circleImageView3"
                        app:layout_constraintStart_toStartOf="@+id/circleImageView3"
                        app:layout_constraintTop_toTopOf="@+id/circleImageView3"
                        app:layout_constraintWidth_percent="0.5490" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/notosanskrmedium"
                    android:includeFontPadding="false"
                    android:letterSpacing="-0.03"
                    android:text="디자인"
                    android:textColor="@color/gray_content"
                    android:textSize="13dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout15" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/btn_front_dev"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="14dp"
            android:layout_marginBottom="80dp"
            app:cardCornerRadius="10dp"
            android:outlineSpotShadowColor="@color/gray_shadow"
            android:outlineAmbientShadowColor="@color/gray_shadow"
            app:cardElevation="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:0.9855"
            app:layout_constraintStart_toStartOf="@+id/btn_plan"
            app:layout_constraintTop_toBottomOf="@id/btn_plan"
            app:layout_constraintWidth_percent="0.3833">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout16"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginTop="30dp"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_percent="0.3695">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/circleImageView4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@color/red_light_btn"
                        tools:layout_editor_absoluteX="0dp"
                        tools:layout_editor_absoluteY="0dp" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:src="@drawable/ic_front_dev"
                        app:layout_constraintBottom_toBottomOf="@+id/circleImageView4"
                        app:layout_constraintDimensionRatio="1:1"
                        app:layout_constraintEnd_toEndOf="@+id/circleImageView4"
                        app:layout_constraintStart_toStartOf="@+id/circleImageView4"
                        app:layout_constraintTop_toTopOf="@+id/circleImageView4"
                        app:layout_constraintWidth_percent="0.5490" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/notosanskrmedium"
                    android:includeFontPadding="false"
                    android:letterSpacing="-0.03"
                    android:text="프론트엔드 개발"
                    android:textColor="@color/gray_content"
                    android:textSize="13dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout16" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/btn_back_dev"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:cardCornerRadius="10dp"
            android:outlineSpotShadowColor="@color/gray_shadow"
            android:outlineAmbientShadowColor="@color/gray_shadow"
            app:cardElevation="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/btn_front_dev"
            app:layout_constraintDimensionRatio="1:0.9855"
            app:layout_constraintStart_toStartOf="@+id/btn_design"
            app:layout_constraintWidth_percent="0.3833">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout17"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginTop="30dp"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_percent="0.3695">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/circleImageView5"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@color/red_light_btn" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:src="@drawable/ic_back_dev"
                        app:layout_constraintBottom_toBottomOf="@+id/circleImageView5"
                        app:layout_constraintDimensionRatio="1:1"
                        app:layout_constraintEnd_toEndOf="@+id/circleImageView5"
                        app:layout_constraintStart_toStartOf="@+id/circleImageView5"
                        app:layout_constraintTop_toTopOf="@+id/circleImageView5"
                        app:layout_constraintWidth_percent="0.5490" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/notosanskrmedium"
                    android:includeFontPadding="false"
                    android:letterSpacing="-0.03"
                    android:text="백엔드 개발"
                    android:textColor="@color/gray_content"
                    android:textSize="13dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout17" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="33dp"
        android:src="@drawable/indicator_3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1:0.17"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintWidth_percent="0.113" />

    <TextView
        android:id="@+id/btn_finish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        android:fontFamily="@font/notosanskrmedium"
        android:includeFontPadding="false"
        android:letterSpacing="-0.03"
        android:text="완료"
        android:textColor="@color/red_main"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView12" />

</androidx.constraintlayout.widget.ConstraintLayout>